<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>Controller.AdminDataController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TherapistController</servlet-name>
        <servlet-class>Controller.TherapistController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientController</servlet-name>
        <servlet-class>Controller.PatientController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addTherapistController</servlet-name>
        <servlet-class>Controller.addTherapistController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addAdminAccount</servlet-name>
        <servlet-class>Controller.addAdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteAdminController</servlet-name>
        <servlet-class>Controller.deleteAdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForumQuestionController</servlet-name>
        <servlet-class>Controller.ForumQuestionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForumAnswerController</servlet-name>
        <servlet-class>Controller.ForumDetailsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForumController</servlet-name>
        <servlet-class>Controller.ForumController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientDataController</servlet-name>
        <servlet-class>Controller.PatientDataController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnswerController</servlet-name>
        <servlet-class>Controller.AnswerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>Controller.LogoutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TherapistController</servlet-name>
        <url-pattern>/TherapistController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientController</servlet-name>
        <url-pattern>/PatientController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addTherapistController</servlet-name>
        <url-pattern>/addTherapistController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addAdminAccount</servlet-name>
        <url-pattern>/addAdminAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteAdminController</servlet-name>
        <url-pattern>/deleteAdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForumQuestionController</servlet-name>
        <url-pattern>/ForumQuestionController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForumAnswerController</servlet-name>
        <url-pattern>/ForumAnswerController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForumController</servlet-name>
        <url-pattern>/ForumController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientDataController</servlet-name>
        <url-pattern>/PatientDataController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnswerController</servlet-name>
        <url-pattern>/AnswerController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
